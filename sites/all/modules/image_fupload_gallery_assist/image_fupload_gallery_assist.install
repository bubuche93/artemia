/**
 * @file
 * Install  SWF-Upload functionality.
 */

/**
 * Implementation of hook_install().
 */
function image_fupload_gallery_assist_install() {
  $data = array();
  $data['gafu_maxfilesize'] = 10000;
  $data['gafu_uploadlimit'] = 100;
  variable_set('image_fupload_gallery_assist_data', $data);
}
